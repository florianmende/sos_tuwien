# Dataset Characteristics and Record Sets
:credit-g-dataset 
    schema:keywords "credit_scoring", "Data Science", "Economics", "finance_problem", "Statistics", "uci", "OpenML100" ;
    dct:subject "Credit Risk Assessment", "Financial Analysis", "Binary Classification" ;
    schema:variableMeasured :checking_status, :duration, :credit_history, :purpose, :credit_amount, 
                            :savings_status, :employment, :installment_commitment, :personal_status,
                            :other_parties, :residence_since, :property_magnitude, :age,
                            :other_payment_plans, :housing, :existing_credits, :job,
                            :num_dependents, :own_telephone, :foreign_worker, :class ;
    cr:recordSet :data-file-recordset ;
    schema:distribution :credit-g-parquet-file .

